


<%= form_for(@pro, html: {class: 'form-horizontal'} )  do |f| %>
  
  <%= render :partial => 'common/errordb', :locals => { :obj => @pro }%>


  <div class="form-group">
    <%= f.label '项目名称', class: 'control-label col-sm-2' %>

    <div class="col-sm-10">
    <%= f.text_field :name, class: 'form-control'%>
    </div>
  </div>
  <div class="form-group">
    <%= f.label '测试地址', class: 'control-label col-sm-2' %>
    <div class="col-sm-10">
    <%= f.text_field :localhost, class: 'form-control' %>
    </div>
  </div>
  <div class="form-group">
    <%= f.label '线上地址', class: 'control-label col-sm-2'%>
    <div class="col-sm-10">
    <%= f.text_field :url, class: 'form-control' %>
    </div>
  </div>

  <div class="form-group">
    <%= f.label '分支名称', class: 'control-label col-sm-2' %>
    <div class="col-sm-10">
    <%= f.text_field :branch, class: 'form-control' %>
    </div>
  </div>

  <div class="form-group">
    <%= f.label '备注', class: 'control-label col-sm-2' %>
    <div class="col-sm-10">
    <%= f.text_area :des, class: 'form-control' %>
    <span class="help-block">可填写帐号等信息。</span>
    </div>
  </div>

  <div class="form-group">

    <div class="col-sm-offset-2 col-sm-10">
      <%= f.submit '提交', class: 'btn btn-primary'%>
    </div>
  </div>

<% end %>
