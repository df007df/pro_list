<%== renderTitle 'show pro'%>

<form class="form-horizontal">

	<div class="form-group">
	<label for="" class="col-sm-2 control-label">项目名称</label>

	<div class="col-sm-10">
		<%= @pro.name %>
	</div>
	</div>

	

	<div class="form-group">
	<label for="" class="col-sm-2 control-label">测试地址</label>

	<div class="col-sm-10">
		<%= @pro.localhost %>
	</div>
	</div>

	<div class="form-group">
	<label for="" class="col-sm-2 control-label">测试Ip</label>

	<div class="col-sm-10">
		<%= @pro.testip %>
	</div>
	</div>

	<div class="form-group">
	<label for="" class="col-sm-2 control-label">线上地址</label>

	<div class="col-sm-10">
		<%= @pro.url %>
	</div>
	</div>

	<div class="form-group">
	<label for="" class="col-sm-2 control-label">分支名称</label>

	<div class="col-sm-10">
		<%= @pro.branch %>
	</div>
	</div>

	<div class="form-group">
	<label for="" class="col-sm-2 control-label">备注</label>

	<div class="col-sm-10">
		<%= @pro.des %>
	</div>
	</div>
</form>


<%if isAdmin?%>
<%= link_to 'Edit', edit_pro_path(@pro) %> |
<%end%>

<%= link_to 'Back', pros_path %>
