<%== renderTitle 'show pro'%>

<form class="form-horizontal">

	<div class="form-group">
	<label for="" class="col-sm-2 control-label">Name</label>

	<div class="col-sm-10">
		<%= @pro.name %>
	</div>
	</div>

	

	<div class="form-group">
	<label for="" class="col-sm-2 control-label">Localhost</label>

	<div class="col-sm-10">
		<%= @pro.localhost %>
	</div>
	</div>

	<div class="form-group">
	<label for="" class="col-sm-2 control-label">Url</label>

	<div class="col-sm-10">
		<%= @pro.url %>
	</div>
	</div>

	<div class="form-group">
	<label for="" class="col-sm-2 control-label">Branch</label>

	<div class="col-sm-10">
		<%= @pro.branch %>
	</div>
	</div>

	<div class="form-group">
	<label for="" class="col-sm-2 control-label">Des</label>

	<div class="col-sm-10">
		<%= @pro.des %>
	</div>
	</div>
</form>

<%= link_to 'Edit', edit_pro_path(@pro) %> |
<%= link_to 'Back', pros_path %>
