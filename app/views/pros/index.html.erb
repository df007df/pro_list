<%== renderTitle 'Listing pros'%>

<a class="btn btn-primary" href="<%= new_pro_path %>"><i class="icon-pencil icon-white"></i> 增加项目</a>
<br>
<br>


<table class="table table-striped table-bordered table-hover">
  <thead>
    <tr>
      <th>项目名称</th>
      <th>项目分支</th>
      <th>测试地址</th>
      <th>线上地址</th>
      <th>备注</th>
      <th>编辑</th>
     
    </tr>
  </thead>

  <tbody>

    <%if @pros.empty?%>
      <tr class=info>
        <td colspan=5 class="text-center">
          无数据
        </td>
      </tr>

    <%end%>
    
    <%= render :partial => "pros/common/tr3", :collection => @pros, :as => :pro%>

  </tbody>
</table>

<%= paginate @pros %>





